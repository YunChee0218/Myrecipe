<h2 class="page-header"><%= category.name %> </h2>
<div class="btn-row">
    <a class="btn btn-primary" href="/categories/<%= category.id %>/edit">Edit</a>
    <%- include('../partials/deleteform', { url: `/categories/${category.id}`}) %> 
</div>
<% if(recipesByCategory.length > 0){ %> 
    <h2 class="page-header">Recipes By Category</h2>
    <%- include('../partials/recipeGrid', { recipes: recipesByCategory, large: true }) %> 
<% } %> 